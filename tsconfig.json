{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "esnext",
        "target": "es2020",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "react"
    },
    "include": [
        "assets/**/*"
    ]
}
